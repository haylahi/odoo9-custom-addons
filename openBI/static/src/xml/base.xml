<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="newGraphView.buttons">
        <div class="btn-group btn-group-sm">
            <button class="btn btn-default fa fa-bar-chart-o oe-bar-mode" title="Bar Chart"></button>
            <button class="btn btn-default fa fa-line-chart oe-line-mode" title="Line Chart"></button>
            <button class="btn btn-default fa fa-pie-chart oe-pie-mode" title="Pie Chart"></button>
            <button class="btn btn-default fa fa-map-marker oe-geo-mode" title="Geo Chart"></button>
        </div>
        <div class="btn-group btn-group-sm">
            <button class="btn btn-default dropdown-toggle" data-toggle="dropdown">
                Measure <span class="caret"/>
            </button>
            <ul class="oe-measure-list dropdown-menu">
                <li t-foreach="measures" t-as="measure" t-att-data-field="measure[0]">
                    <a><t t-esc="measure[1].string"/></a>
                </li>
                <li class="divider"></li>
                <li data-field="__count__"><a>Count</a></li>
            </ul>
        </div>
    </t>
    <t t-name="GeoChartView">
        <div class="mapView">
            <div class="row">
                <div class="col-md-4 col-sm-12">
                    <div class="panel panel-default">
                        <div class="panel-heading" aria-expanded="false" data-toggle="collapse" data-target="#display-settings">
                            Display Settings
                            <i class="fa fa-chevron-right pull-right"></i>
                            <i class="fa fa-chevron-down pull-right"></i>
                        </div>
                        <div id="display-settings" class="collapse">
                            <ul class="list-group form-inline">
                                <li class="list-group-item">Display Mode 
                                    <select class="pull-right" name="displayMode">
                                        <option value="regions">Regions</option>
                                        <option value="markers">Markers</option>
                                        <option value="text">Text</option>
                                    </select>
                                </li>
                                <li class="list-group-item">Continent 
                                    <t t-call="openBI.continents"></t>
                                </li>
                                <li class="hidden list-group-item">Sub Continent 
                                    <t t-call="openBI.subcontinents"></t>
                                </li>
                                <li class="hidden list-group-item">Country 
                                    <t t-call="openBI.countries"></t>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="panel panel-default">
                        <div class="panel-heading" aria-expanded="false" data-toggle="collapse" data-target="#color-settings">
                            Color Settings
                            <i class="fa fa-chevron-right pull-right"></i>
                            <i class="fa fa-chevron-down pull-right"></i>
                        </div>
                        <div id="color-settings" class="collapse">
                            <ul class="list-group">
                                <li class="list-group-item">Background-color<input class="pull-right" name="backgroundColor" type="color" value="#FFFFFF" /></li>
                                <li class="list-group-item">Dataless region color<input class="pull-right" name="datalessRegionColor" type="color" value="#F5F5F5" /></li>
                                <li class="list-group-item">Color Axis From<input class="pull-right" name="colorAxisFrom" type="color" value="#EEE5DB" /></li>
                                <li class="list-group-item">Color Axis To<input class="pull-right" name="colorAxisTo" type="color" value="#267114" /></li>
                            </ul>
                        </div>
                    </div>
                    <div class="button-action row">
                        <div class="col-xs-12">
                            <button id="printMap" class="btn btn-default btn-md pull-right">Print</button>
                        </div>
                    </div>
                </div>
                <div class="col-md-7 col-sm-12">
                    <div id="map_div"></div>
                    <div id="map-table" class="panel panel-default">
                        <div class="panel-heading" aria-expanded="false" data-toggle="collapse" data-target="#map-data-display">
                            Data Display
                            <i class="fa fa-chevron-right pull-right"></i>
                            <i class="fa fa-chevron-down pull-right"></i>
                        </div>
                        <div id="map-data-display" class="collapse">
                            <div id="table_div"></div>
                        </div>
                    </div>
                </div>
                <div class="col-md-1"></div>
            </div>
        </div>
    </t>
